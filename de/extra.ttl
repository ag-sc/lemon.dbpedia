:lexicon a lemon:Lexicon ;
    lemon:language "de" ;
    lemon:entry :hoch,
                :gross,
                :lang,
                :weit,
                :tief,
                :entdecken,
                :entdeckung,
                :gruenden,
                :gruendenAm,
                :produzieren,
                :herstellen .


## height, length, area ## 

:hoch a lemon:Word ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm [ lemon:writtenRep "hoch"@de ] ;
  lemon:synBehavior [ rdf:type lexinfo:AdjectivePredicateFrame ;
                      lexinfo:copulativeSubject :arg1hoch ;
                      lexinfo:attributiveArg    :arg1hoch ],                      
                    [ rdf:type lexinfo:AdjectiveScaleFrame ;
                      lexinfo:copulativeSubject   :arg1hoch ;
                      lexinfo:adverbialComplement :arg2hoch ];
  lemon:sense [ lemon:reference [ rdfs:subClassOf oils:CovariantScalar ;
                                  oils:boundTo <http://dbpedia.org/ontology/height> ;
                                  oils:degree oils:strong ] ;
                lemon:isA :arg1hoch  ] ,
              [ lemon:reference <http://dbpedia.org/ontology/height> ;
                lemon:subjOfProp :arg1hoch ;
                lemon:objOfProp  :arg2hoch ] , 
              [ lemon:reference [ rdfs:subClassOf oils:CovariantScalar ;
                                  oils:boundTo <http://dbpedia.org/ontology/elevation> ;
                                  oils:degree oils:strong ] ;
                lemon:isA :arg1hoch  ] ,
              [ lemon:reference <http://dbpedia.org/ontology/elevation> ;
                lemon:subjOfProp :arg1hoch ;
                lemon:objOfProp  :arg2hoch ] .

